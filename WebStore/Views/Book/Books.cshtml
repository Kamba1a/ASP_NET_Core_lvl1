@model IEnumerable<BookViewModel>

@{
    ViewData["Title"] = "Книги";
    Layout = null;
}

<h1>Список книг</h1>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id .</td>
            <td>@item.Title</td>
            <td>@Html.ActionLink("Подробнее..", "BookDetails", new { id = item.Id })</td>
        </tr>
    }
</table>